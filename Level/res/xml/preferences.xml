<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
	<ListPreference
        android:key="pref_speed"
        android:title="@string/pref_speed"
        android:dialogTitle="@string/pref_speedType"
        android:entries="@array/pref_speed_entries"
        android:entryValues="@array/pref_speed_value"
        
        android:defaultValue="0" android:summary="@string/pref_speed_summary"/>
	<CheckBoxPreference android:summaryOn="@string/level_armed" android:summaryOff="@string/level_disarmed" android:key="pref_sound" android:title="@string/pref_sound_title"/>
</PreferenceScreen>